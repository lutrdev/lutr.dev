{
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "extends": [
    "sznm/react",
    "plugin:react/jsx-runtime",
    "plugin:@next/next/recommended"
  ],
  "rules": {
    "react/no-unescaped-entities": 0,
    "consistent-return": "off",
    "default-case": "off"
  },
  "ignorePatterns": [
    "**/*.config.js"
  ]
}
